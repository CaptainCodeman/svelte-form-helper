{
  "name": "svelte-form-helper",
  "version": "0.2.0",
  "type": "module",
  "keywords": [
    "svelte",
    "form",
    "forms",
    "validation",
    "valid",
    "submit",
    "state"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://captaincodeman.github.io/svelte-form-helper/",
  "repository": {
    "type": "git",
    "url": "https://github.com/captaincodeman/svelte-form-helper.git"
  },
  "author": {
    "name": "Simon Green",
    "email": "simon@captaincodeman.com",
    "url": "https://www.captaincodeman.com/"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "tsup",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/forms": "^0.5.3",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "postcss-load-config": "^4.0.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  }
}